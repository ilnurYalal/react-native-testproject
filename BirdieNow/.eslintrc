{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": false,
    "node": true
  },
  "globals": {
    "window": true,
    "fetch": true,
    "navigator": true
  },
  "plugins": [
    "react",
    "react-native"
  ],
  "rules": {
    "comma-dangle": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-console": 1,
    "react-native/no-unused-styles": 2,
    "import/no-unresolved": 0,
    "import/first": 0,
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "no-return-assign": 0,
    "react/jsx-boolean-value": 0,
    "one-var-declaration-per-line": 0,
    "one-var": 0,
    "arrow-parens": 0,
    "global-require": 0,
    "class-methods-use-this": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-indent": 0,
    "react/forbid-prop-types": 0,
    "max-len": 0
  }
}
